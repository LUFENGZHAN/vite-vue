{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"baseUrl": "./",
		"paths": {
			"@/*": ["./src/*"]
		},
		"moduleResolution": "Node",
		"resolveJsonModule": true,
		"strict": true
	},
	"include": ["src/**/*.ts", "src/**/*.d.ts", "src/**/*.tsx", "src/**/*.vue", "auto-imports.d.ts", "components.d.ts", "auto-api.ts"]
}
